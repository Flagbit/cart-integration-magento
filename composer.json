{
  "name": "shopgate/cart-integration-magento",
  "description": "Integrates the Magento 1 shopping cart solution with Shopgate",
  "homepage": "https://github.com/shopgate/cart-integration-magento",
  "license": "Apache-2.0",
  "type": "magento-module",
  "config": {
    "platform": {
      "php": "5.3.3"
    },
    "vendor-dir": "vendor"
  },
  "authors": [
    {
      "name": "Shopgate",
      "email": "interfaces@shopgate.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "shopgate/cart-integration-sdk": "~2.9.70"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "suggests": {
    "magento-hackathon/magento-composer-installer": "~3.0.0"
  },
  "extra":{
      "map": [
        ["src/app/etc/modules/Shopgate_Framework.xml", "app/etc/modules/Shopgate_Framework.xml"],
        ["src/app/code/community/Shopgate/*", "app/code/community/Shopgate"],
        ["src/app/design/adminhtml/default/default/layout/shopgate.xml", "app/design/adminhtml/default/default/layout/shopgate.xml"],
        ["src/app/design/adminhtml/default/default/template/shopgate", "app/design/adminhtml/default/default/template/shopgate"],
        ["src/app/design/frontend/base/default/layout/shopgate.xml", "app/design/frontend/base/default/layout/shopgate.xml"],
        ["src/app/design/frontend/base/default/template/shopgate", "app/design/frontend/base/default/template/shopgate"],
        ["src/app/locale/de_AT/Shopgate_Framework.csv", "app/locale/de_AT/Shopgate_Framework.csv"],
        ["src/app/locale/de_CH/Shopgate_Framework.csv", "app/locale/de_CH/Shopgate_Framework.csv"],
        ["src/app/locale/de_DE/Shopgate_Framework.csv", "app/locale/de_DE/Shopgate_Framework.csv"],
        ["src/media/shopgate", "media/shopgate"],
        ["src/skin/adminhtml/default/default/shopgate", "skin/adminhtml/default/default/shopgate"],
        ["src/var/connect/shopgate_module.xml", "var/connect/shopgate_module.xml"],
        ["../cart-integration-sdk", "lib/Shopgate/cart-integration-sdk"]
      ]
    },
  "keywords": [
    "mobile",
    "app"
  ]
}
